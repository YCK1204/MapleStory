table CharacterAbility {
	STR:uint16;
	DEX:uint16;
	INT:uint16;
	LUK:uint16;
}

table CharacterInfo {
	char_id:uint64;
	char_type:uint8;
	level:uint16;
	name:string;
	ability:CharacterAbility;
}

table CharacterInfoDetail {
	prev_info:CharacterInfo;
	last_pos:int32;
	hp:int32;
	mp:int32;
	exp:int32;
	money:int32;
}