.Button-Base {
    flex-grow: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    background-color: rgba(0, 0, 0, 0);
    width: 19%;
    height: 15%;
    position: absolute;
    bottom: 16%;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.Button-Ok {
    background-image: resource('PrevInGame/Imgs/Common/Button/BtOk.normal.0');
}

.Button-Ok:hover {
    background-image: resource('PrevInGame/Imgs/Common/Button/BtOk.mouseOver.0');
}

.Button-Ok:active {
    background-image: resource('PrevInGame/Imgs/Common/Button/BtOk.mouseOver.0');
}

.Button-Cancel {
    background-image: resource('PrevInGame/Imgs/Common/Button/BtCancel.normal.0');
}

